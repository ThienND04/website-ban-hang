<h1>Home</h1>

<div class="mt-4">
    <div class="row row-cols-5">
      {{#each products}}
      <div class="card" style="width: 18rem;">
        <img src="http://127.0.0.1:3003/images/{{this.images.[0]}}" class="card-img-top" alt="{{this.name}}">
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
        </div>
      </div>
    {{/each}}
    </div>
</div>

